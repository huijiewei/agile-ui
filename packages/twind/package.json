{
  "name": "@agile-ui/twind",
  "version": "1.0.0",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prod": "tsup src/index.ts --format cjs,esm --dts --minify --clean"
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huijiewei/agile-ui.git",
    "directory": "packages/twind"
  },
  "peerDependencies": {
    "twind": ">=1",
    "@twind/preset-autoprefix": ">=1",
    "@twind/preset-tailwind": ">=1",
    "@twind/preset-ext": ">=1"
  },
  "dependencies": {
    "@agile-ui/utils": "*"
  },
  "devDependencies": {
    "twind": "next",
    "@twind/preset-autoprefix": "next",
    "@twind/preset-tailwind": "next",
    "@twind/preset-ext": "next"
  }
}
